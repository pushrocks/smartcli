{
  "name": "smartcli",
  "version": "1.0.15",
  "description": "nodejs wrapper for CLI related tasks",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "test": "(npmts)",
    "testm": "(cd ts/compile && gulp) && (node test.js jazz jam --awesome)",
    "devTest": "(npm test) && (node test.js --test true)",
